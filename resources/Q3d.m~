function [prcstut,prcgauss] = Q3d(X)

meanG = mean(X)

Bmisample = zeros(100,50);
Lowstut = zeros(100,1);
Lowgauss = zeros(100,1);
upstut = zeros(100,1);
upgauss = zeros(100,1);
i =1;
while(i<=100)
    Bmisample(i,:) = iid_sample(X,50);
    meanBMI(i,1) = mean(Bmisample(i,:));
    [Lowstut(i,1),upstut(i,1)] = stutdent_interval(meanBMI(i,1),0.95);
    [Lowgauss(i,1),upgauss(i,1)] = gauss_interval(meanBMI(i,1),0.95);
    i = i+1;
end

cptstut = 0;
cptgauss =0;
i = 1;

while(i<=100)
    if( meanG < upstut || meanG > Lowstut )
        cptstut = cptstut+1;
    end
    if( meanG < upgauss || meanG > Lowgauss )
        cptgauss = cptgauss+1;
    end
    i = i+1;
end





end
